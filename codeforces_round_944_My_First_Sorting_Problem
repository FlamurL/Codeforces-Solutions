#include <bits/stdc++.h>
#define ll long long int
#define fr(i,n) for(int i=0;i<n;i++)
#define pb push_back
using namespace std;
void fast()
{
    cin.tie(0);
    cout.tie(0);
}
int main()
{
    fast();
    int t;
    cin>>t;
    while(t--)
    {
      int x,y;
      cin>>x>>y;
      if(x>y)
      cout<<y<<" "<<x;
      else
      cout<<x<<" "<<y;
      cout<<endl;
 
    }
 
  
}
