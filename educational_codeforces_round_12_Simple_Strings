#include<bits/stdc++.h>
using namespace std;
int main()
{
  

    string s;
   cin>>s;
    
        for(long long int i=1;i<s.size();i++){
           if(s[i]==s[i-1]){
            for(long long int j='a';j<='z';j++){
                if(s[i+1]!=j && s[i-1]!=j)
                {
                    s[i]=j;
                    break;
                }
            }
           }
        }
        cout<<s<<endl;
    

}
