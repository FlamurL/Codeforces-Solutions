#include<iostream>
 
using namespace std;
 
int main()
{
   int t;
   cin>>t;
   while(t--){
       string s;
       cin>>s;
       int cntb=0, cnta=0;
       for(int i=0;i<s.length();i++){
           if(s[i]=='A')
           cnta++;
           else
           cntb++;
       }
       if(cnta<cntb)
       cout<<"B";
       else
       cout<<"A";
       cout<<endl;
   }
}
