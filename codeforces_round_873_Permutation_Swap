#include <bits/stdc++.h>
using namespace std;
#define ll long long int
#define fr(i,n) for(int i=0;i<n;i++)
int main() {
    int t;
    cin>>t;
    while(t--){
  int n;
  cin>>n;
  int p[n];
  int sol=0;
  fr(i,n){
      cin>>p[i];
sol=__gcd(sol,abs(p[i]-i-1));
  }
  
  cout<<sol<<endl;
 
  
    }
  
}
