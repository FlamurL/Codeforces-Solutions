#include <bits/stdc++.h>
using namespace std;
int main() {
int t;
cin>>t;
while(t--){
int a,b,c;
cin>>a>>b>>c;
int s=c-b;
int k=b-s;
int s1=b-a;
int k1=s1+b;
int s2=(c-a)/2;
int k2=a+s2;
if(k%a==0&&k>0){
 
    cout<<"YES";
}
else if(k1%c==0&&k1>0){
       
    cout<<"YES";
}
else if((c-a)%2==0&&k2%b==0&&k2>0){
        
    cout<<"YES";
}
else
cout<<"NO";
 
cout<<endl;
 
}
}
