#include <bits/stdc++.h>
#define ll long long int
#define fr(i,n) for(int i=0;i<n;i++)
#define f(i,n) for(int i=1;i<n;i++)
using namespace std;
int main() {
int t;
cin>>t;
while(t--){
    int n,m;
    cin>>n>>m;
    string a,b;
    cin>>a>>b;
    int z=0;
    fr(i,m){
        if(b[i]==a[z]){
            z++;
        }
    }
    cout<<z<<endl;
    
}
}
